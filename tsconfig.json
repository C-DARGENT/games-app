{
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@modelModelPortfolios/*": [
        "src/app/modules/model-portfolio/modules/model-model-portfolios/*"
      ],
      "exceljs": ["node_modules/exceljs/dist/exceljs.min"],
      "@auth/*": ["src/app/modules/auth/*"],
      "@core/*": ["src/app/core/*"],
      "@shared/*": ["src/app/shared/*"],
      "@app/*": ["src/app/*"],
      "@src/*": ["src/*"]
    }
  }
}
